        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" 
	xmlns:j="jelly:core" 
	xmlns:g="glide" 
	xmlns:j2="null" 
	xmlns:g2="null">
	<tr id="ph_macro_row">
		<td class="ph_macro">
			<g:evaluate var="jvar_gr" object="true">
				var gr = new GlideRecord("notify_number");
				gr.addQuery("active", true);
				gr.addQuery("owner","NexmoDriver");
				gr.query();
				gr;
			</g:evaluate>
			<div class="ph_data">

				<div id="phs">
					<table class="table table-hover original-table list-grid table-striped" tabindex="0">
						<thead class="table-body-headers">
							<tr class="table-header">
								<th scope="col" class="table-column-header">${gs.getMessage('Phone number')}</th>
								<!-- <th scope="col" class="table-column-header">${gs.getMessage('Capabilities')}</th> -->
								<!-- <th scope="col" class="territory table-column-header">${gs.getMessage('Territory')}</th> -->
								<!-- <th scope="col" class="ngrp table-column-header">${gs.getMessage('Notify group')}</th> -->
								<th scope="col" class="advSms table-column-header">${gs.getMessage('Advanced SMS features')}</th>
								<th scope="col" class="configStatus table-column-header">${gs.getMessage('Advanced VOICE features')}</th>
							</tr>
						</thead>
						<tbody id="result" class="list_table_tbody">
							<j:while  test="${jvar_gr.next()}">
								<tr>
									<td> ${jvar_gr.getValue('number')} </td>
									<!-- <td>${jvar_gr.getValue('notify_group')}</td> -->
									<td>${jvar_gr.getValue('has_sms_out')} </td>
									<td>${jvar_gr.getValue('has_voice_out')}</td>
									<td>${jvar_gr.getValue('onwer')} </td>
								</tr>

							</j:while>
						</tbody>
					</table>
				</div>


			</div>
		</td>
	</tr>


</j:jelly>]]></xml>
    </sys_ui_macro>
</record_update>
